{
  "name": "flattenizer",
  "description": "Small library for flattening and unflattening objects.",
  "version": "2.0.1",
  "author": "Sean Hellebusch <sahellebusch@gmail.com>",
  "bugs": {
    "url": "https://github.com/sahellebusch/flattenizer/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "gh-pages": "^2.1.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2"
  },
  "homepage": "https://github.com/sahellebusch/flattenizer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "main": "./dist/flattenizer.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahellebusch/flattenizer.git"
  },
  "scripts": {
    "build": "tsc",
    "docs:build": "npx typedoc --out api",
    "docs:publish": "npm run docs:build && npx gh-pages -a -d api -m \"Update: Docs\"",
    "lint": "tslint src/flattenizer.ts spec/flattenizer.spec.ts",
    "lint:fix": "tslint src/flattenizer.ts spec/flattenizer.spec.ts --fix",
    "postpublish": "npm run docs:deploy",
    "prepublish": "npm run build",
    "test": "jest"
  }
}
