{
  "name": "flattenizer",
  "description": "Small library for flattening and unflattening objects.",
  "version": "2.0.2",
  "author": "Sean Hellebusch <sahellebusch@gmail.com>",
  "bugs": {
    "url": "https://github.com/sahellebusch/flattenizer/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "homepage": "https://github.com/sahellebusch/flattenizer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "./dist/flattenizer.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahellebusch/flattenizer.git"
  },
  "scripts": {
    "build": "tsc",
    "docs:build": "typedoc --out api",
    "docs:publish": "npm run docs:build && gh-pages -a -d api -m \"Update: Docs\"",
    "lint": "eslint src/flattenizer.ts src/test/flattenizer.ts",
    "lint:fix": "npm run lint -- --fix",
    "postpublish": "npm run docs:deploy",
    "prepare": "npm run build",
    "test": "jest"
  }
}
