{
  "name": "flattenizer",
  "description": "Small library for flattening and unflattening objects.",
  "version": "2.0.2",
  "author": "Sean Hellebusch <sahellebusch@gmail.com>",
  "bugs": {
    "url": "https://github.com/sahellebusch/flattenizer/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-cypress": "2.7.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "gh-pages": "^2.1.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2"
  },
  "homepage": "https://github.com/sahellebusch/flattenizer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "./dist/flattenizer.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahellebusch/flattenizer.git"
  },
  "scripts": {
    "build": "tsc",
    "docs:build": "typedoc --out api",
    "docs:publish": "npm run docs:build && gh-pages -a -d api -m \"Update: Docs\"",
    "lint": "eslint src/flattenizer.ts src/test/flattenizer.ts",
    "lint:fix": "npm run lint -- --fix",
    "postpublish": "npm run docs:deploy",
    "prepare": "npm run build",
    "test": "jest"
  }
}
